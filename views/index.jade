extends layout

block content
	.site-wrapper
		.site-wrapper-inner
			.cover-container
				.masthead.clearfix
					.inner
						h3.masthead-brand #{title}
						nav
							ul.nav.masthead-nav
								li(class='active')
									a(href='/') Monitor
								li
									a(href='/top') Top
				.inner.cover
					h1.cover-heading Links
					-
					each val, index in links
						p.lead
							a(href=val) #{index}
				.mastfoot
					.inner
						p Brought to you by Magic :D



